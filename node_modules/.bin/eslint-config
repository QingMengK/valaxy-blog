#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_4ea0a136884b31c3241d8e7774901951/node_modules/@antfu/eslint-config/bin/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_4ea0a136884b31c3241d8e7774901951/node_modules/@antfu/eslint-config/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_4ea0a136884b31c3241d8e7774901951/node_modules/@antfu/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_4ea0a136884b31c3241d8e7774901951/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_4ea0a136884b31c3241d8e7774901951/node_modules/@antfu/eslint-config/bin/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_4ea0a136884b31c3241d8e7774901951/node_modules/@antfu/eslint-config/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_4ea0a136884b31c3241d8e7774901951/node_modules/@antfu/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_4ea0a136884b31c3241d8e7774901951/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
else
  exec node  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
fi
