#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/vite-ssg@0.23.7_critters@0._437f5c9f8c33c5a55b1eca54547d9687/node_modules/vite-ssg/bin/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/vite-ssg@0.23.7_critters@0._437f5c9f8c33c5a55b1eca54547d9687/node_modules/vite-ssg/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/vite-ssg@0.23.7_critters@0._437f5c9f8c33c5a55b1eca54547d9687/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/vite-ssg@0.23.7_critters@0._437f5c9f8c33c5a55b1eca54547d9687/node_modules/vite-ssg/bin/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/vite-ssg@0.23.7_critters@0._437f5c9f8c33c5a55b1eca54547d9687/node_modules/vite-ssg/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/vite-ssg@0.23.7_critters@0._437f5c9f8c33c5a55b1eca54547d9687/node_modules:/mnt/c/DEV/valaxy-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite-ssg@0.23.7_critters@0._437f5c9f8c33c5a55b1eca54547d9687/node_modules/vite-ssg/bin/vite-ssg.js" "$@"
else
  exec node  "$basedir/../../../../../vite-ssg@0.23.7_critters@0._437f5c9f8c33c5a55b1eca54547d9687/node_modules/vite-ssg/bin/vite-ssg.js" "$@"
fi
